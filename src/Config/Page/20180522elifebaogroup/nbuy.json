{
    "basicInfo": {
        "detailsPage": "nbuy",
        "productId": "20180522elifebaogroup",
        "title": "平安e生保团体版"
    },
    "htmlBrick": [
        "nbuy_holder",
        "nbuy_insured",
        "nbuy_policy",
        "nbuy_favoree",
        "nbuy_supple_info",
        "nbuy_clause",
        "nbuy_footer"
    ],
    "bindEvent": {
        "e_nbuy_validate": {},
        "e_buy_fold": {},
        "e_nbuy_getcode": {},
        "e_nbuy_clause": {},
        "nbuyPurchase": {}
    },
    "serviceLogic": "",
    "dataFlow": "",
    "renderData": {
        "item": {
            "holder": {
                "title": "被保险人",
                "objArray": [{
                    "title": "姓名",
                    "type": "input",
                    "input": {
                        "id": "holder_userName",
                        "minlength": 2,
                        "maxlength": 32,
                        "placeholder": "请输入姓名",
                        "data-belong": "policyHolderUser",
                        "data-type": "userName",
                        "data-verify": "userName"
                    }
                }, {
                    "title": "证件类型",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "holder_certiType",
                        "data-value": "0",
                        "value": "身份证",
                        "placeholder": "身份证",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiType"
                    }
                }, {
                    "title": "证件号码",
                    "type": "input",
                    "input": {
                        "id": "holder_certiNo",
                        "placeholder": "请输入证件号码",
                        "oninput": "if(value.length>18)value=value.slice(0,18)",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiNo",
                        "data-verify": "certiNo"
                    }
                }, {
                    "title": "电子邮箱",
                    "type": "input",
                    "input": {
                        "id": "holder_email",
                        "placeholder": "请输入邮箱地址",
                        "oninput": "if(value.length>30)value=value.slice(0,30)",
                        "data-belong": "policyHolderUser",
                        "data-type": "email",
                        "data-verify": "email"
                    }
                },{
                    "title": "为谁投保",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "relaId",
                        "value": "本人",
                        "data-id": "00",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }]
            },
            "policy": {
                "title": "其他信息",
                "objArray": [{
                    "title": "起保日期",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "policyBeginDate",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }]
            }
        }
    }
}