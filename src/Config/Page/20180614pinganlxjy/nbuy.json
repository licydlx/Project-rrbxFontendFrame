{
    "basicInfo": {
        "detailsPage": "nbuy",
        "productId": "20180614pinganlxjy",
        "title": "乐享教育"
    },
    "htmlBrick": [
        "nbuy_policy",
        "nbuy_holder",
        "nbuy_lxjy",
        "nbuy_pazh",
        "nbuy_favoree",
        "nbuy_supple_info",
        "nbuy_clause",
        "nbuy_footer"
    ],
    "bindEvent": {
        "e_nbuy_validate": {},
        "e_buy_fold": {},
        "e_nbuy_getcode": {},
        "e_nbuy_clause": {},
        "nbuyPurchase": {}
    },
    "serviceLogic": "",
    "dataFlow": "",
    "renderData": {
        "item": {
            "policy": {
                "title": "基本信息",
                "objArray": [{
                    "title": "起保日期",
                    "type": "choose",
                    "choose": {
                        "id": "policyBeginDate",
                        "placeholder": "请选择起保日期",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }, {
                    "title": "终止日期",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "policyEndDate",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }]
            },
            "holder": {
                "title": "投保人信息",
                "objArray": [{
                    "title": "营业执照(请上传)",
                    "type": "uploadImg",
                    "uploadImg": {
                        "formId":"uploadForm",
                        "inputId":"uploadImg"
                    }
                }, {
                    "title": "联系人",
                    "type": "input",
                    "input": {
                        "id": "holder_userName",
                        "minlength": 2,
                        "maxlength": 32,
                        "placeholder": "请输入联系人姓名",
                        "data-belong": "policyHolderUser",
                        "data-type": "userName",
                        "data-verify": "userName"
                    }
                }, {
                    "title": "证件类型",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "holder_certiType",
                        "data-value": "00",
                        "value": "身份证",
                        "placeholder": "身份证",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiType"
                    }
                }, {
                    "title": "证件号码",
                    "type": "input",
                    "input": {
                        "id": "holder_certiNo",
                        "placeholder": "请输入证件号码",
                        "oninput": "if(value.length>18)value=value.slice(0,18)",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiNo",
                        "data-verify": "certiNo"
                    }
                }, {
                    "title": "电子邮箱",
                    "type": "input",
                    "input": {
                        "id": "holder_email",
                        "placeholder": "请输入邮箱地址",
                        "oninput": "if(value.length>30)value=value.slice(0,30)",
                        "data-belong": "policyHolderUser",
                        "data-type": "email",
                        "data-verify": "email"
                    }
                },{
                    "title": "是否需要发票",
                    "type": "choose",
                    "choose": {
                        "id": "invoice",
                        "value": "不需要",
                        "data-id": "0",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }]
            },
            "lxjy":{
                "title": "被保险人信息(以下方式任选一种)",
                "exampleImg":"https://m1.renrenbx.com/rrbxcdn/rrbx/pinganlxjy/exampleImg.jpg"
            },
            "pazh":{
                "title": "平安账户信息",
                "bankImg":"https://m1.renrenbx.com/rrbxcdn/rrbx/pinganlxjy/pingan.jpg",
                "bankTemplate":"https://m1.renrenbx.com/rrbxcdn/rrbx/pinganlxjy/bankTemplate.jpg"
            }
        }
    }
}