{
    "basicInfo": {
        "detailsPage": "nbuy",
        "productId": "20180525shrsdjgprepayment",
        "title": "大金刚预付费"
    },
    "htmlBrick": [
        "nbuy_holder",
        "nbuy_insured",
        "nbuy_favoree",
        "nbuy_supple_info",
