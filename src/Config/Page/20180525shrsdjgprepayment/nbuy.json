    },
    "serviceLogic": "",
    "dataFlow": "",
    "renderData": {
        "item": {
            "holder": {
                "title": "投保人",
                "objArray": [{
                    "title": "姓名",
                    "type": "input",
                    "input": {
                        "id": "holder_userName",
                        "minlength": 2,
                        "maxlength": 32,
                        "placeholder": "请输入姓名",
                        "data-belong": "policyHolderUser",
                        "data-type": "userName",
                        "data-verify": "userName"
                    }
                }, {
                    "title": "证件类型",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "holder_certiType",
                        "data-value": "0",
                        "value": "身份证",
                        "placeholder": "身份证",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiType"
                    }
                }, {
                    "title": "证件号码",
                    "type": "input",
                    "input": {
                        "id": "holder_certiNo",
                        "placeholder": "请输入证件号码",
                        "oninput": "if(value.length>18)value=value.slice(0,18)",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiNo",
                        "data-verify": "certiNo"
                    }
                }, {
                    "title": "证件有效期",
                    "type": "singleSelect",
                    "data-id": "holderCardValid",
                    "singleSelect": [{
                        "data-id": "1",
                        "text": "非长期"
                    },{
                        "data-id": "0",
                        "text": "长期"
                    }]
                }, {
                    "title": "有效止期",
                    "type": "choose",
                    "choose": {
                        "id": "holderCardValid",
                        "value": "2020-01-01",

                        "data-id": "2020-01-01",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }, {
                    "title": "居住省市",
                    "type": "choose",
                    "choose": {
                        "id": "holderArea",
                        "placeholder": "请选择居住省市",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",

                        "data-belong": "extraParams",
                        "data-key": "holderResidentProvince,holderResidentCity,holderResidentCounty",
                        "data-type": "area",
                        "data-verify": "area"
                    }
                }, {
                    "title": "详细地址",
                    "type": "input",
                    "input": {
                        "id": "holderAddress",
                        "placeholder": "请输入详细地址",
                        "minlength": 2,
                        "maxlength": 32,

                        "data-belong": "policyHolderUser",
                        "data-type": "address",
                        "data-verify": "all"
                    }
                }, {
                    "title": "个人职业",
                    "type": "choose",
                    "choose": {
                        "id": "holderOccupationCode",
                        "placeholder": "请选择个人职业",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }, {
                    "title": "邮箱地址",
                    "type": "input",
                    "input": {
                        "id": "holder_email",
                        "placeholder": "请输入邮箱地址",
                        "oninput": "if(value.length>30)value=value.slice(0,30)",
                        "data-belong": "policyHolderUser",
                        "data-type": "email",
                        "data-verify": "email"
                    }
                }]
            },
            "insured": {
                "title": "被保险人",
