                    }
                }, {
                    "title": "详细地址",
                    "type": "input",
                    "input": {
                        "id": "insuredAddress",
                        "placeholder": "请输入详细地址",
                        "minlength": 2,
                        "maxlength": 32,

                        "data-belong": "insuredUser",
                        "data-type": "address",
                        "data-verify": "address"
                    }
                }, {
                    "title": "个人职业",
                    "type": "choose",
                    "choose": {
                        "id": "insureOccupationCode",
                        "placeholder": "请选择个人职业",

                        "unselectable": "no",
