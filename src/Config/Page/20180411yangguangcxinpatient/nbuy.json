{
    "basicInfo": {
        "detailsPage": "nbuy",
        "productId": "20180411yangguangcxinpatient",
        "title": "阳光财险-住院保"
    },
    "htmlBrick": [
        "nbuy_holder",
        "nbuy_insured",
        "nbuy_favoree",
        "nbuy_supple_info",
        "nbuy_clause",
        "nbuy_footer"
    ],
    "bindEvent": {
        "e_nbuy_validate": {},
        "e_buy_fold": {},
        "e_nbuy_getcode": {},
        "e_nbuy_clause": {},
        "nbuyPurchase": {}
    },
    "serviceLogic": "",
    "dataFlow": "",
    "renderData": {
        "item": {
            "holder": {
                "title": "投保人",
                "objArray": [{
                    "title": "姓名",
                    "type": "input",
                    "input": {
                        "id": "holder_userName",
                        "minlength": 2,
                        "maxlength": 32,
                        "placeholder": "请输入姓名",
                        "data-belong": "policyHolderUser",
                        "data-type": "userName",
                        "data-verify": "userName"
                    }
                }, {
                    "title": "证件类型",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "holder_certiType",
                        "data-value": "00",
                        "value": "身份证",
                        "placeholder": "身份证",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiType"
                    }
                }, {
                    "title": "证件号码",
                    "type": "input",
                    "input": {
                        "id": "holder_certiNo",
                        "placeholder": "请输入证件号码",
                        "oninput": "if(value.length>18)value=value.slice(0,18)",
                        "data-belong": "policyHolderUser",
                        "data-type": "certiNo",
                        "data-verify": "certiNo"
                    }
                }, {
                    "title": "邮箱地址",
                    "type": "input",
                    "input": {
                        "id": "holder_email",
                        "placeholder": "请输入邮箱地址",
                        "oninput": "if(value.length>30)value=value.slice(0,30)",
                        "data-belong": "policyHolderUser",
                        "data-type": "email",
                        "data-verify": "email"
                    }
                }, {
                    "title": "居住地址",
                    "type": "input",
                    "input": {
                        "id": "holder_address",
                        "placeholder": "请输入详细地址",
                        "minlength": 2,
                        "maxlength": 32,

                        "data-belong": "policyHolderUser",
                        "data-type": "address",
                        "data-verify": "all"
                    }
                }, {
                    "title": "邮政编码",
                    "type": "input",
                    "input": {
                        "id": "holder_insuredZip",
                        "placeholder": "请输入邮政编码",
                        "minlength": 2,
                        "maxlength": 6,

                        "data-belong": "extraParams",
                        "data-type": "insuredZip",
                        "data-verify": "postcode"
                    }
                }]
            },
            "insured": {
                "title": "被保险人",
                "objArray": [{
                    "title": "为谁投保",
                    "type": "choose",
                    "choose": {
                        "id": "relaId",
                        "value": "本人",
                        "data-id": "1",

                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly"
                    }
                }, {
                    "title": "姓名",
                    "type": "input",
                    "input": {
                        "id": "insured_userName",
                        "minlength": 2,
                        "maxlength": 32,
                        "placeholder": "请输入姓名",
                        "data-belong": "insuredUser",
                        "data-type": "userName",
                        "data-verify": "userName"
                    }
                }, {
                    "title": "证件类型",
                    "type": "inputDefault",
                    "inputDefault": {
                        "id": "insured_certiType",
                        "data-value": "00",
                        "value": "身份证",
                        "placeholder": "身份证",
                        "unselectable": "no",
                        "onfocus": "this.blur()",
                        "readonly": "readonly",
                        "data-belong": "insuredUser",
                        "data-type": "certiType"
                    }
                }, {
                    "title": "证件号码",
                    "type": "input",
                    "input": {
                        "id": "insured_certiNo",
                        "placeholder": "请输入证件号码",
                        "oninput": "if(value.length>18)value=value.slice(0,18)",
                        "data-belong": "insuredUser",
                        "data-type": "certiNo",
                        "data-verify": "certiNo"
                    }
                }]
            }
        }
    }
}