<section class="background">
	<section class="box">
		<div class="header">
			<i class="tag"></i>
			<%= "保障计划" %>
		</div>

		<% if(item) { %>
			<% for(var i=0;i < item.length;i++){ %>
				<% if(item[i].type == "choose") { %>
					<section class="item <%= item[i].type %>">
						<label class="title"><%= item[i].title %></label>
						<input class="content"
							<% for(var x in item[i].choose){ %>
								<%= x+"="+item[i].choose[x] %>
							<% } %>
						>
						<i class="icon">
							<img class="iconImg" src="https://m1.renrenbx.com/rrbxcdn/rrbx/arrows.png">
						</i>
					</section>
				<% } %>

				<% if(item[i].type == "singleSelect") { %>
					<section class="item <%= item[i].type %>">
						<label class="title"><%= item[i].title %></label>
						<ul class="content">
							<% for(var y=0;y < item[i].tab.length;y++){ %>
							<li><a
								<% if(y==0){ %>
									class="active"
								<% } %>
								<%= "data-id"+"="+item[i].tab[y]["data-id"] %>
								><%= item[i].tab[y].text %></a></li>
							<% } %>
						</ul>
					</section>
				<% } %>
				
				<% if(item[i].type == "multiSelect") { %>
					<section class="item <%= item[i].type %>">
						<label class="title"><%= item[i].title %></label>
						<ul class="content" id="<%= item[i].id %>">
							<% for(var y=0;y < item[i].tab.length;y++){ %>
							<li><a
								<%= "data-id"+"="+item[i].tab[y]["data-id"] %>
								><%= item[i].tab[y].text %></a></li>
							<% } %>
						</ul>
					</section>
				<% } %>
			<% } %>
		<% } %>
	</section>
</section>