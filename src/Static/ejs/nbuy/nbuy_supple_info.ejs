<section class="box suppleInfo">
	<a class="header">
		<i class="arrows"></i>
		补充信息
	</a>
	<div class="itemBox">
		<section class="item input">
			<label class="title">手&#12288;&#12288;机</label>
			<input id="supple-info-tel" type="tel" data-verify="phoneno" data-belong="extraParams" data-type="phone" oninput="if(value.length>11)value=value.slice(0,11)"></input>
		</section>
		<section class="item input">
			<label class="title">验&#8194;证&#8194;码</label>
			<input id="supple-info-code" type="tel" data-belong="extraParams" data-type="captcha" data-verify="authCode"></input>
			<span id="getCode" class="getcode">获取验证码</span>
		</section>
	</div>

</section>